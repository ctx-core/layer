{
	"name": "@ctx-core/layer",
	"version": "13.0.1",
	"description": "ctx-core layer",
	"keywords": [
		"ctx-core",
		"layer"
	],
	"homepage": "https://github.com/ctx-core/layer#readme",
	"bugs": {
		"url": "https://github.com/ctx-core/layer/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ctx-core/layer/issues"
	},
	"license": "Apache-2.0",
	"author": "Brian Takita",
	"type": "module",
	"exports": {
		".": {
			"types": "./src/index.ts",
			"import": "./dist/index.js"
		},
		"./package.json": "./package.json"
	},
	"scripts": {
		"build": "npm run compile",
		"clean": "rm -rf dist",
		"compile": "swc src --out-dir dist --copy-files --source-maps --config-file .swcrc",
		"exec": "$@",
		"prepublishOnly": "npm run clean && npm run build"
	},
	"dependencies": {
		"@ctx-core/array": "workspace:^23.0.33",
		"@ctx-core/error": "workspace:^11.1.9",
		"@ctx-core/function": "workspace:^20.2.12",
		"@ctx-core/object": "workspace:^22.0.0",
		"@ctx-core/store": "workspace:^27.0.47",
		"svelte": "^3.44.3"
	},
	"devDependencies": {
		"@swc/cli": "^0.1.55",
		"@swc/core": "^1.2.120",
		"typescript": "next"
	},
	"publishConfig": {
		"access": "public",
		"cache": "~/.npm"
	}
}
